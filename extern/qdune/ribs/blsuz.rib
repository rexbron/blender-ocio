Hider "hidden" "jitter" [1]
Display "Blender Suzanne" "framebuffer" "rgba"
#Display "blender_suzanne.tga" "file" "rgb"
Projection "perspective" "fov" [50]

PixelSamples 4 4
Format 800 800 1

Translate 0 0 5
Rotate -110 1 0 0
Rotate -120 0 0 1

WorldBegin

	LightSource "pointlight" 2 "intensity" [50] "from" [0 -6 6]
	LightSource "distantlight" 3 "intensity" [0.2] "from" [0 6 6] "to" [0 0 0]

	Surface "glossy" "Kd" [1] "Ks" [1]

	TransformBegin
	Scale 1.5 1.5 1.5
	Rotate 90  1 0 0
	Rotate -30  0 1 0
	ReadArchive "suzanne.rib"
	TransformEnd

WorldEnd
